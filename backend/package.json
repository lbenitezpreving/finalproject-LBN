{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend para TaskDistributor - Sistema de gestión y distribución de tareas",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev --name init",
    "prisma:studio": "prisma studio",
    "prisma:reset": "prisma migrate reset",
    "db:seed": "node prisma/seed.js",
    "db:partition": "node scripts/run-partition.js",
    "db:setup": "npm run prisma:migrate && npm run db:partition && npm run db:seed",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix"
  },
  "keywords": [
    "nodejs",
    "express",
    "prisma",
    "postgresql",
    "jwt",
    "api",
    "taskdistributor"
  ],
  "author": "TaskDistributor Team",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^6.8.2",
    "backend": "file:",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^8.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "prisma": "^6.8.2",
    "supertest": "^7.0.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
